  "scripts": {
    "start": "node ./scripts/webpack/environments/development/server.js --project=HCA --env=local",
    "build:Debug": "copyfiles -f  ./src/Project/HCA/public/**/* ./src/Project/HCA/build/ && cross-env NODE_ENV=production webpack && stylelint \"**/*.scss\"",
    "sc:codegen": "gulp generate-ts --gulpfile ./scripts/code-generation/codeGen.js",
    "sc:codegen:commerce": "gulp generate-ts-commerce --gulpfile ./scripts/code-generation/codeGen.js",
    "test": "jest --no-cache --config ./scripts/jest/jest.config.js || exit 0",
    "test-watch": "jest --watch --notify --config ./scripts/jest/jest.config.js",
    "test-cover": "jest --coverage --config ./scripts/jest/jest.config.js",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "tslint --project ./tsconfig.json",
    "lint:styles": "stylelint \"**/*.scss\"",
    "prettify": "prettier --write './src/**/*.{ts,tsx,css}'",
    "generate-doc": "typedoc --out CodeDocs --ignoreCompilerErrors",
    "storybook": "start-storybook -c ./scripts/storybook"
